<tbody id="list-search-results">
    <% lists.each do |list| %>
      <tr>
        <td><%= list.title %></td>
        <td>
          <% list.categories.each do |c| %>
            <%= c.title %>
          <% end %>
          <%# list.category_ids.each do |c| %>
            <%#= Category.find(c).title %>
          <%# end %>
          <%= %>
        </td>
        <td><%= list.created_at %></td>
        <td><%= list.updated_at %></td>
        <td><%= list.num_copies %></td>
        <td><%= list.text_messages.length %></td>
        <td><%= link_to 'Show', list %></td>
        <td><%#= link_to 'Edit', edit_list_path(list) %></td>
        <td><%#= link_to 'Destroy', list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
</tbody>